{
  "name": "rails-diff",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for rails-diff goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "fix": "run-s fix:*",
    "fix:eslint": "run-s \"lint:eslint --fix\"",
    "fix:prettier": "run-s fix:prettier:*",
    "fix:prettier:hbs": "prettier --parser glimmer --write \"**/*.hbs\"",
    "fix:prettier:js": "prettier --write \"**/*.{js,ts}\"",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:eslint": "eslint --cache --ext js,ts --max-warnings=0 .",
    "lint:prettier": "run-s lint:prettier:*",
    "lint:prettier:hbs": "prettier --list-different --parser glimmer \"**/*.hbs\"",
    "prettier": "prettier",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.1",
    "@glimmer/tracking": "^1.0.1",
    "@types/ember": "^3.16.1",
    "@types/ember-data": "^3.16.5",
    "@types/ember-qunit": "^3.4.12",
    "@types/ember-resolver": "^5.0.10",
    "@types/ember__test-helpers": "^1.7.3",
    "@types/qunit": "^2.9.5",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "babel-eslint": "^8.2.6",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.6.0",
    "ember-cli": "~3.21.2",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-babel": "^7.22.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-mirage": "^1.1.8",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.0",
    "ember-cli-typescript": "^4.0.0",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-data": "~3.21.0",
    "ember-decorators": "^6.1.1",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.0.2",
    "ember-intl": "^5.5.0",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-semantic-test-helpers": "^1.0.0",
    "ember-source": "~3.21.1",
    "ember-template-lint": "^2.11.0",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-ember": "^8.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "qunit-dom": "^1.4.0",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  }
