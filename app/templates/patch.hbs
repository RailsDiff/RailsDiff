{{page-title
  (t
    "title.patch"
    source=this.model.sourceVersion
    target=this.model.targetVersion
  )
  prepend=false
  separator=(t "title-separator")
}}

{{#each this.model.patch.diffs as |diff|}}
  <Diff
    @diff={{diff}}
    @linkedAnchor={{this.linkedAnchor}}
    @sourceVersion={{this.model.sourceVersion}}
    @targetVersion={{this.model.targetVersion}}
  />
{{/each}}